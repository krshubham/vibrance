<!-- Page Specific Scripts -->

    <script type="text/javascript">

    $(document).ready(function() {
        $().UItoTop({ easingType: 'easeOutQuart' });

        // Sticky Nav Function

        var stickyNav = function() {

            var scrollTop = $(window).scrollTop();
            var stickyNavTop = 1;

            if (scrollTop >= stickyNavTop) {
                $('.menu').addClass('sticky');
                $('.under-nav').addClass('margin');
            }
            else {
                $('.menu').removeClass('sticky');
                $('.under-nav').removeClass('margin');
            }

        };

        // RESIZE TO FULLSCREEN

        function resizeFullscreen() {

            var windowHeight = $(window).height();
            var windowWidth = $(window).width();
            var navHeight = $('.menu').height();

            if (windowHeight < 400) {
                windowHeight = 400;
            }
            sectionHeight = windowHeight;

            $('.fullscreen').css({ height: sectionHeight - navHeight });

        }

        //SHARE ICONS

        function shareURLS() {

            var currentURL = $(location).attr('href');

            $('#facebook-share-btn').attr('href', 'http://www.facebook.com/sharer.php?u=' + currentURL);
            $('#linkedin-share-btn').attr('href', 'https://www.linkedin.com/shareArticle?mini=true&url=' + currentURL + '&title=Custom%20Content%20by%20Brave%20People&summary=&source=');
            $('#twitter-share-btn').attr('href', 'https://twitter.com/share?url=' + currentURL + '&amp;text=Custom%20Content%20by%20Brave%20People&amp;hashtags=brvppl');

        }

        //FADE IN

        function fade() {
            $('.fade-element').each(function() {

                var objectBottom = $(this).offset().top + 300;
                var windowBottom = $(window).scrollTop() + $(window).innerHeight();

                if (objectBottom < windowBottom) {
                    $(this).addClass('fadein');
                }

                /* if (objectBottom > windowBottom) {
                    $(this).removeClass('fadeleft');
                }*/
            });
        }

        // Function Calls

        setTimeout(stickyNav, 1000);
        resizeFullscreen();
        fade();

        $(window).load(function() {
            shareURLS();
            fade();
        });

        $(window).scroll(function() {
           stickyNav();
           fade();
        });

        $(window).resize(function() {
            stickyNav();
            resizeFullscreen();
            fade();
        });

    });


    </script>


