    	$(document).ready(function() {
    		$().UItoTop({ easingType: 'easeOutQuart' });
    	});
    </script>

    <script type="text/javascript">
    $(document).ready(function() {

        // Sticky Nav Function

        var stickyNav = function() {

            var scrollTop = $(window).scrollTop();
            var stickyNavTop = 1;

            if (scrollTop >= stickyNavTop) {
                $('.menu').addClass('sticky');
                $('.under-nav').addClass('margin');
            }
            else {
                $('.menu').removeClass('sticky');
                $('.under-nav').removeClass('margin');
            }

        };

        // Function Calls

        setTimeout(stickyNav, 1000);

        $(window).scroll(function() {
           stickyNav();
        });

        $(window).resize(function() {
            stickyNav();
        });

    });
    </script>

<!-- Google Maps -->

    <script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDY0kkJiTPVd2U7aTOAwhc9ySH6oHxOIYM&sensor=false">
    </script>

    <script>
    var myCenter=new google.maps.LatLng(27.957510, -82.434041);

    function initialize()
    {
    	var styles = [
    	{

    		featureType: "natural",
    		stylers: [
    			{ hue: '#333333' },
    			{ saturation: -100 },
    			{ invert_lightness: true }

    		]
    	},{
    		featureType: "landscape",
    		stylers: [
    			{ color: '#333333' },
    			{ saturation: 0 }
    		]
    	},{
    		featureType: "road".highway,
    		stylers: [
    			{ hue: '#898989' },
    			{ saturation: -100 }

    		]
    	},{
    		featureType: "road.local",
    		elementType: "geometry",
    		stylers: [
    			{ color: '#666666' },
    		]
    	},{
    		featureType: "road",
    		elementType: "labels",
    		stylers: [
    			{ hue: '#242424' },
    		]
    	},{
    		featureType: "road",
    		elementType: "icons",
    		stylers: [
    			{ hue: '#ffffff' }
    		]
    	},{
    		featureType: "road.arterial",
    		stylers: [
    			{ hue: '#999999' },
    			{ gamma: 1 },
    			{ visibility: 'off' }

    		]
    	},{
    		featureType: "building",
    		elementType: "labels",
    		stylers: [
    			{ hue: '#ffffff' },
    			{ lightness: 20 }
    		]
    	},{
    		featureType: "poi", //points of interest
    		elementType: "geometry",
    		stylers: [
    			{ visibility: 'off' }
    		]
    	},{
    		featureType: "water", //water
    		stylers: [
    			{ hue: '#acb1b1' },
    			{ saturation: -100 }

    		]
    	}
    ];

    var mapProp = {
      center:myCenter,
      zoom:15,
      disableDefaultUI:true,
      mapTypeId:google.maps.MapTypeId.ROADMAP ,
      styles: styles
      };

    var map=new google.maps.Map(document.getElementById("map"),mapProp);

    // Map Activate / Scroll Over Functions
    google.maps.event.addListenerOnce(map, 'idle', function() {
        $('#map').prepend('<div id="map-activate" onclick="javascript:$(this).hide();"></div>');
    });

    var marker=new google.maps.Marker({
      position:myCenter,
      icon:'/assets/images/mapmarker.png'
      });

    marker.setMap(map);
    }

    google.maps.event.addDomListener(window, 'load', initialize);

    </script>


